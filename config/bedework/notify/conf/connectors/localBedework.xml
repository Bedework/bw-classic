<?xml version="1.0" encoding="UTF-8" ?>

<notify-connector xmlns="http://bedework.org/ns/"
                 type="org.bedework.notifier.cnctrs.bedework.BedeworkConnectorConfig">
  <notificationDirHref>http://localhost:8080/pubcaldav/user/notify-user/Notifications/</notificationDirHref>
  <connectorClassName>org.bedework.notifier.cnctrs.bedework.BedeworkConnector</connectorClassName>
  <keepAliveInterval>300</keepAliveInterval>
  <mbeanClassName>org.bedework.notifier.cnctrs.bedework.BedeworkConnConf</mbeanClassName>
  <name>localBedework</name>
  <readOnly>false</readOnly>
  <retryInterval>10</retryInterval>
  <token>abcd</token>
  <trustLastmod>true</trustLastmod>
</notify-connector>
